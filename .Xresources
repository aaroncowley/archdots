Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.dpi: 96

! URxvt

! General settings
URxvt.saveLines: 2000
URxvt.scrollBar: False
URxvt.scrollstyle: rxvt
URxvt.termName: xterm-256color
URxvt.urgentOnBell: True
URxvt.cursorBlink: False

!annoying yellow box

URxvt.iso14755 : false

! Extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
URxvt.urlLauncher: xdg-open
URxvt.urlButton: 1
URxvt.underlineURLs: True


! Keybindings
!URxvt.keysym.Shift-Control-V: perl:clipboard:copy
!URxvt.keysym.Shift-Control-C: perl:clipboard:paste

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard



URxvt*buffered: false
URxvt.font:xft:DejaVu Sans Mono-10
URxvt.boldFont:xft:DejaVu Sans Mono-10
URxvt.letterSpace: 0

urxvt*depth: 32
! special

URxvt.background:               [80]#1b1918
URxvt.foreground:               #dddddd
!URxvt.foreground:               #42f4aa
URxvt.colorBD:                  #CFCFCF
URxvt.colorUL:                  #969696
URxvt.colorIT:                  #686868
