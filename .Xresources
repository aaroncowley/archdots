Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.dpi: 96

! URxvt

! General settings
URxvt.saveLines: 2000
URxvt.scrollBar: False
URxvt.scrollstyle: rxvt
URxvt.termName: xterm-256color
URxvt.urgentOnBell: True
URxvt.cursorBlink: False

!annoying yellow box

URxvt.iso14755 : false

! Extensions
URxvt.perl-ext-common: default,clipboard,keyboard-select,url-select
URxvt.urlLauncher: xdg-open
URxvt.urlButton: 1
URxvt.underlineURLs: True


! Keybindings
!URxvt.keysym.Shift-Control-V: perl:clipboard:copy
!URxvt.keysym.Shift-Control-C: perl:clipboard:paste

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard



URxvt*buffered: false
URxvt*font: xft:Terminess Powerline:size=11
URxvt.boldFont: xft:Terminess Powerline:size=11
URxvt.letterSpace: 0

urxvt*depth: 32
! terminal colours 
URxvt.background:               [90]#1b1b1b
URxvt.foreground:               #FFFFFF
URxvt.colorBD:                  #CFCFCF
URxvt.colorUL:                  #969696
URxvt.colorIT:                  #686868
!Black
*color0:                        #3D3D3D
*color8:                        #4D4D4D 
!Red
*color1:                        #6673BF
*color9:                        #899AFF 
!Green
